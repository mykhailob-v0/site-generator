{
  "name": "index-html-generator",
  "version": "2.0.0",
  "description": "Production-ready AI-powered HTML generator for high-quality gambling/betting sites with enhanced error handling, logging, and testing infrastructure",
  "main": "cli.js",
  "bin": {
    "html-generator": "./cli.js",
    "html-generator-refactored": "./cli-refactored.js"
  },
  "scripts": {
    "start": "node cli.js",
    "start:refactored": "node cli-refactored.js",
    "generate": "node cli.js generate",
    "generate:refactored": "node cli-refactored.js generate",
    "status": "node cli.js status",
    "status:refactored": "node cli-refactored.js status",
    "validate": "node cli.js validate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "dev": "nodemon cli.js",
    "install-global": "npm install -g ."
  },
  "keywords": [
    "html-generator",
    "seo",
    "gambling",
    "betting",
    "openai",
    "gpt-5",
    "dalle-3",
    "turkish",
    "ymyl",
    "eeat"
  ],
  "author": "HTML Generator",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.1.2",
    "commander": "^12.1.0",
    "dotenv": "^16.6.1",
    "inquirer": "^10.2.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.63.0",
    "sharp": "^0.33.5",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nock": "^13.3.3",
    "nodemon": "^3.1.4",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "local"
  },
  "homepage": ".",
  "preferGlobal": true,
  "files": [
    "cli.js",
    "services/",
    "config/",
    "prompts/",
    "schemas/",
    "src/",
    "README.md",
    "PRODUCTION_REFACTOR_PLAN.md"
  ]
}
